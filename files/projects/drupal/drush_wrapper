#!/usr/bin/env sh
# Generated by "Salt-Stack"
# A drush wrapper for local project environment.

# Configuration
DRUSH_CMD="{project_lib_root}/vendor/drush/drush/drush"
WWW_DIR="{project_web_root}"
DRUPAL_URI="http://{project_domain}"
COMMAND_DIRS="{project_lib_root}/vendor/drush/drush/commands"
DRUSH_PHP=""

# Drush wrapper
export DRUSH_PHP
$DRUSH_CMD --include=$COMMAND_DIRS --root=$WWW_DIR --uri=$DRUPAL_URI "$@"
